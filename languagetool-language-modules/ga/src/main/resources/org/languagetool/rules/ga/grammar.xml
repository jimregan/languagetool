                    <marker>
                        <token>le</token>
                        <token>mise</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “liomsa” a scríobh.</message>
                <example correction='Dúirt sé liomsa'>Dúirt sé <marker>le mise</marker></example>
                <example>Dúirt sé liomsa</example>
            </rule>
            <rule id="LE_TUSA_LEATSA" name="le + tusa = leatsa">
                <pattern>
                    <marker>
                        <token>le</token>
