            </rule>
            <rule id="LE_EISEAN_LEIS-SEAN" name="le + eisean = leis-sean">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>eisean|seisean</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “leis-sean” a scríobh.</message>
                <example correction='Dúirt sé leis-sean'>Dúirt sé <marker>le eisean</marker></example>
                <example>Dúirt sé leis-sean</example>
            </rule>
            <rule id="LE_ISE_LEISE" name="le + ise = léise">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>ise|sise</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “léise” a scríobh.</message>
                <example correction='Dúirt sé léise'>Dúirt sé <marker>le ise</marker></example>
                <example>Dúirt sé léise</example>
            </rule>
            <rule id="LE_SINNE_LINNE" name="le + sinne = linne">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>sinne|muidne|muide</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “linne” a scríobh.</message>
                <example correction='Dúirt sé linne'>Dúirt sé <marker>le sinne</marker></example>
                <example>Dúirt sé linne</example>
            </rule>
            <rule id="LE_SIBHSE_LIBHSE" name="le + sibhse = libhse">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>sibhse</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “libhse” a scríobh.</message>
                <example correction='Dúirt sé libhse'>Dúirt sé <marker>le sibhse</marker></example>
                <example>Dúirt sé libhse</example>
            </rule>
            <rule id="LE_IADSAN_LEOSAN" name="le + iadsan = leosan">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>iadsan|siadsan</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “leosan” a scríobh.</message>
                <example correction='Dúirt sé leosan'>Dúirt sé <marker>le iadsan</marker></example>
                <example>Dúirt sé leosan</example>
            </rule>
        </rulegroup>
    </category>
</rules>
