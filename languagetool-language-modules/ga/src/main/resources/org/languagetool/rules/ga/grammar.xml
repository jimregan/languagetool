<!DOCTYPE rules [
        <!ENTITY nbsp " ">
        <!ENTITY apost "['`’]">
        ]>
<rules lang="ga" xsi:noNamespaceSchemaLocation="../../../../../../../../../languagetool-core/src/main/resources/org/languagetool/rules/rules.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <category id="TEST" name="Test" type="typographical">
        <rulegroup id="TESTRULES" name="Test rules">
            <rule id="FEAR_NOT_FEARR" name="fear not fearr">
                <pattern>
                    <token inflected='yes' postag='Verb:.*' postag_regexp="yes"></token>
                    <token>an</token>
                    <marker>
                        <token>fearr</token>
                    </marker>
                </pattern>
                <message>An raibh “fear” ar intinn agat?</message>
                <example correction='bhí an fear ann'>bhí an <marker>fearr</marker> ann</example>
                <example>bhí an fear ann</example>
            </rule>
        </rulegroup>
        <rulegroup id="CONTRACTIONS" name="Contractions">
            <rule id="LE_ME_LIOM" name="le + mé = liom">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>mé</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “liom” a scríobh.</message>
                <example correction='Dúirt sé liom'>Dúirt sé <marker>le mé</marker></example>
                <example>Dúirt sé liom</example>
            </rule>
            <rule id="LE_TU_LEAT" name="le + tú = leat">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token regexp="yes">tú|thú</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “leat” a scríobh.</message>
                <example correction='Dúirt sé leat'>Dúirt sé <marker>le tú</marker></example>
                <example>Dúirt sé leat</example>
            </rule>
            <rule id="LE_E_LEIS" name="le + é = leis">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>é|sé</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “leis” a scríobh.</message>
                <example correction='Dúirt sé leis'>Dúirt sé <marker>le é</marker></example>
                <example>Dúirt sé leis</example>
            </rule>
            <rule id="LE_I_LEI" name="le + í = léi">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>í|sí</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “léi” a scríobh.</message>
                <example correction='Dúirt sé léi'>Dúirt sé <marker>le í</marker></example>
                <example>Dúirt sé léi</example>
            </rule>
            <rule id="LE_SINN_LINN" name="le + sinn = linn">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>sinn|muid</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “linn” a scríobh.</message>
                <example correction='Dúirt sé linn'>Dúirt sé <marker>le sinn</marker></example>
                <example>Dúirt sé linn</example>
            </rule>
            <rule id="LE_SIBH_LIBH" name="le + sibh = libh">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>sibh</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “libh” a scríobh.</message>
                <example correction='Dúirt sé libh'>Dúirt sé <marker>le sibh</marker></example>
                <example>Dúirt sé libh</example>
            </rule>
            <rule id="LE_IAD_LEO" name="le + iad = leo">
                <pattern>
                    <marker>
                        <token>le</token>
                        <token>iad|siad</token>
                    </marker>
                </pattern>
                <message>Ba chóir duit “leo” a scríobh.</message>
                <example correction='Dúirt sé leo'>Dúirt sé <marker>le iad</marker></example>
                <example>Dúirt sé leo</example>
            </rule>
            <rule id="LE_MISE_LIOMSA" name="le + mise = liomsa">
